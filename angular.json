{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "app": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "lnd",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": true,
                        "outputPath": "dist/app",
                        "index": "src/index.html",
                        "main": "src/app.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "src/**/*.!(scss)",
                                "input": "src/assets/",
                                "output": "src/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/ng2-pdfjs-viewer/pdfjs",
                                "output": "/assets/pdfjs"
                            },
                            "src/assets/configurations/config.js",
                            "src/assets/csv/",
                            "src/assets/images/",
                            "src/assets/static-data/",
                            "src/assets/pdfs/",
                            "src/assets/favicon.ico",
                            "src/robots.txt",
                            "src/sitemap.xml"
                        ],
                        "styles": [
                            "src/assets/scss/front.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "100kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        },
                        "preview": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        },
                        "mock": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ]
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "app:build"
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "app:build"
                    },
                    "configurations": {
                        "mock": {
                            "browserTarget": "app:build:mock"
                        }
                    }
                },
                "server": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                        "sourceMap": false,
                        "outputPath": "dist/server",
                        "main": "server.ts",
                        "tsConfig": "src/tsconfig.server.json"
                    },
                    "configurations": {
                        "production": {
                            "outputHashing": "media",
                            "sourceMap": false,
                            "optimization": true
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            {
                                "glob": "src/**/*.!(scss)",
                                "input": "src/assets/",
                                "output": "src/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/ng2-pdfjs-viewer/pdfjs",
                                "output": "/assets/pdfjs"
                            },
                            "src/assets/pdfs/",
                            "src/assets/favicon.ico",
                            "src/robots.txt",
                            "src/sitemap.xml"
                        ],
                        "styles": [
                            "src/assets/scss/front.scss"
                        ],
                        "scripts": [
                            "src/assets/configurations/config.test.js"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "**/src/500/**",
                            "**/src/static/**",
                            "**/src/static.ts"
                        ]
                    }
                },
                "serve-ssr": {
                    "builder": "@nguniversal/builders:ssr-dev-server",
                    "options": {
                        "browserTarget": "app:build",
                        "serverTarget": "app:server"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "app:build:production",
                            "serverTarget": "app:server:production"
                        }
                    }
                },
                "prerender": {
                    "builder": "@nguniversal/builders:prerender",
                    "options": {
                        "browserTarget": "app:build:production",
                        "serverTarget": "app:server:production",
                        "routes": [
                            "/"
                        ]
                    },
                    "configurations": {
                        "production": {}
                    }
                }
            }
        },
        "static": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "lnd",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": true,
                        "outputPath": "dist/static",
                        "index": "src/index.html",
                        "main": "src/static.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "src/**/*.!(scss)",
                                "input": "src/assets/",
                                "output": "src/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/ng2-pdfjs-viewer/pdfjs",
                                "output": "/assets/pdfjs"
                            },
                            "src/assets/configurations/config.js",
                            "src/assets/csv/",
                            "src/assets/pdfs/",
                            "src/assets/static-data/",
                            "src/assets/images/",
                            "src/assets/favicon.ico"
                        ],
                        "styles": [
                            "src/assets/scss/front.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "static:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "static:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "static:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "**/src/500/**",
                            "**/src/app/**",
                            "**/src/app.ts"
                        ]
                    }
                }
            }
        },
        "app-500": {
            "root": "",
            "sourceRoot": "src/500/",
            "projectType": "application",
            "prefix": "lnd",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": true,
                        "outputPath": "dist/500",
                        "index": "src/500/index.html",
                        "main": "",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "src/500/**/*.!(scss)",
                                "input": "src/500/assets/",
                                "output": "src/500/assets/"
                            },
                            "src/500/assets/images/",
                            "src/500/assets/favicon.ico"
                        ],
                        "styles": [
                            "src/500/assets/scss/front.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "app-500:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "app-500:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "app-500:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "**/src/**",
                            "!**/src/500/**"
                        ]
                    }
                }
            }
        },
        "app-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "protractor.conf.js",
                        "devServerTarget": "app:serve:mock"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**",
                            "**/src/500/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "app",
    "cli": {
        "packageManager": "yarn"
    }
}
