{
  "name": "angular-devstack",
  "version": "7.0.0",
  "description": "Lundegaard Angular Devstack",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:app": "yarn build --prod --output-hashing none",
    "build:preview": "yarn build -c preview",
    "build:prod": "yarn build --prod",
    "build:static": "yarn build static --prod --base-href /static/",
    "build:500": "yarn build app-500 --prod --base-href /500/",
    "development": "yarn start",
    "bundle-report-create": "yarn build:app --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scss": "stylelint \"./src/**/*.scss\"",
    "ng": "ng",
    "production": "yarn build:prod",
    "preview": "yarn build:preview",
    "start": "ng serve",
    "start:app": "yarn start --proxy-config proxy-dev.conf.json",
    "start:preview": "yarn start --proxy-config proxy-prev.conf.json",
    "start:test": "yarn start --proxy-config proxy-test.conf.json",
    "start:static": "yarn start static",
    "start:500": "yarn start app-500",
    "start:mock": "yarn start -c mock --proxy-config proxy-mock.conf.json",
    "start:mock-docker": "yarn start -c mock --proxy-config proxy-mock-docker.conf.json",
    "start:ssr": "node dist/server/main.js",
    "test": "ng test app",
    "test:watch": "ng test --watch=true app",
    "build:client-and-server-bundles": "yarn build:app && ng run app:server:production",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "dev:ssr": "yarn ng-high-memory run app:serve-ssr",
    "build:ssr": "yarn build:client-and-server-bundles",
    "prerender": "ng run app:prerender"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@angular/animations": "10.2.5",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/forms": "10.2.5",
    "@angular/localize": "^10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/router": "10.2.5",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-select/ng-select": "^5.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@types/file-saver": "^2.0.0",
    "apollo-angular": "^1.7.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.1.0",
    "core-js": "^2.5.7",
    "cron": "^1.8.2",
    "cross-fetch": "^3.0.4",
    "crypto-js": "^4.0.0",
    "d3": "^5.16.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.2",
    "graphql": "14.0.0",
    "graphql-tag": "^2.10.1",
    "html-entities": "^2.3.2",
    "latinize": "^0.4.1",
    "memory-cache": "^0.2.0",
    "moment": "^2.22.2",
    "ng-recaptcha": "^6.1.0",
    "ngx-bootstrap": "^4.3.0",
    "ngx-cookie": "^4.1.2",
    "parse5": "^6.0.0",
    "ramda": "^0.26.1",
