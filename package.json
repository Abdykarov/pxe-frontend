{
  "name": "angular-devstack",
  "version": "7.0.0",
  "description": "Lundegaard Angular Devstack",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:app": "yarn build --configuration production --output-hashing none",
    "build:preview": "yarn build -c preview",
    "build:prod": "yarn build --configuration production",
    "build:static": "yarn build static --configuration production --base-href /static/",
    "build:500": "yarn build app-500 --configuration production --base-href /500/",
    "development": "yarn start",
    "bundle-report-create": "yarn build:app --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scss": "stylelint \"./src/**/*.scss\"",
    "ng": "ng",
    "production": "yarn build:prod",
    "preview": "yarn build:preview",
    "start": "ng serve",
    "start:app": "yarn start --proxy-config proxy-dev.conf.json",
    "start:preview": "yarn start --proxy-config proxy-prev.conf.json",
    "start:test": "yarn start --proxy-config proxy-test.conf.json",
    "start:static": "yarn start static",
    "start:500": "yarn start app-500",
    "start:mock": "yarn start -c mock --proxy-config proxy-mock.conf.json",
    "start:mock-docker": "yarn start -c mock --proxy-config proxy-mock-docker.conf.json",
    "start:ssr": "node dist/server/main.js",
    "test": "ng test app",
    "test:watch": "ng test --watch=true app",
    "build:client-and-server-bundles": "yarn build:app && ng run app:server:production",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "dev:ssr": "yarn ng-high-memory run app:serve-ssr",
    "build:ssr": "yarn build:client-and-server-bundles",
    "prerender": "ng run app:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.13",
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/forms": "12.2.13",
    "@angular/localize": "^12.2.13",
    "@angular/platform-browser": "12.2.13",
    "@angular/platform-browser-dynamic": "12.2.13",
    "@angular/router": "12.2.13",
    "@apollo/client": "y^3.0.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-select/ng-select": "^7.3.0",
    "@nguniversal/express-engine": "^12.1.3",
    "@types/file-saver": "^2.0.0",
    "apollo-angular": "^2.6.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.1.0",
    "core-js": "^2.5.7",
    "cron": "^1.8.2",
    "cross-fetch": "^3.0.4",
    "crypto-js": "^4.0.0",
    "d3": "^5.16.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.2",
    "graphql": "^15.0.0",
    "html-entities": "^2.3.2",
    "latinize": "^0.4.1",
    "memory-cache": "^0.2.0",
    "moment": "^2.22.2",
    "ng-recaptcha": "^8.0.1",
    "ngx-bootstrap": "^4.3.0",
    "ngx-cookie": "^4.1.2",
    "parse5": "^6.0.0",
    "ramda": "^0.26.1",
    "ramda-extension": "^0.7.2",
    "request": "^2.88.2",
    "rxjs": "^6.5.5",
    "smoothscroll-polyfill": "^0.4.4",
    "stylelint": "^9.5.0",
    "stylelint-scss": "^3.3.0",
    "tslib": "^2.0.0",
    "unfetch": "^4.1.0",
    "unorm": "^1.6.0",
    "uuid": "^3.3.3",
    "vanilla-text-mask": "^5.1.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "xml2js": "^0.4.23",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@angular/language-service": "12.2.13",
    "@angular/platform-server": "^12.2.13",
    "@nguniversal/builders": "^9.1.0",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "compression-webpack-plugin": "6.1.1",
    "domino": "^2.1.3",
    "husky": "^4.2.5",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.18.11",
    "protractor": "~7.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "~6.1.0",
    "typescript": "4.3.5",
    "webpack-cli": "^3.3.0"
  },
  "browser": {
    "crypto": false
  },
  "resolutions": {
    "webpack": "^5.0.0"
  }
}
