        "@angular/router": "12.2.13",
        "@apollo/client": "^3.4.16",
        "@auth0/angular-jwt": "^5.0.2",
        "@ng-select/ng-select": "^7.3.0",
        "@nguniversal/express-engine": "^12.1.3",
        "@types/file-saver": "^2.0.0",
        "animate.css": "^4.1.1",
        "apollo-angular": "^2.6.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.1.0",
        "core-js": "^3.19.1",
        "cron": "^1.8.2",
        "cross-fetch": "^3.0.4",
        "crypto-js": "^4.0.0",
        "d3": "^5.16.0",
        "express": "^4.15.2",
        "file-saver": "^2.0.2",
        "graphql": "^15.0.0",
        "html-entities": "^2.3.2",
        "latinize": "^0.4.1",
        "memory-cache": "^0.2.0",
        "moment": "^2.22.2",
        "ng-recaptcha": "^8.0.1",
        "ngx-bootstrap": "^4.3.0",
