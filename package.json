{
  "name": "angular-devstack",
  "version": "7.0.0",
  "description": "Lundegaard Angular Devstack",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:app": "yarn build --prod",
    "build:preview": "yarn build -c preview",
    "build:prod": "yarn build --prod",
    "build:static": "yarn build static --prod --base-href /static/",
    "build:500": "yarn build app-500 --prod --base-href /500/",
    "development": "yarn start",
    "e2e": "ng e2e",
    "lang": "ng xi18n --output-path assets/locale --i18n-locale cs",
    "lint": "ng lint",
    "lint:scss": "stylelint \"./src/**/*.scss\"",
    "ng": "ng",
    "production": "yarn build:prod",
    "preview": "yarn build:preview",
    "start": "ng serve",
    "start:app": "yarn start --proxy-config proxy-dev.conf.json",
    "start:static": "yarn start static",
    "start:500": "yarn start app-500",
    "start:mock": "ng serve -c mock --proxy-config proxy-mock.conf.json",
    "start:mock-docker": "ng serve -c mock --proxy-config proxy-mock-docker.conf.json",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.10",
    "@angular/common": "7.2.10",
    "@angular/compiler": "7.2.10",
    "@angular/core": "7.2.10",
    "@angular/forms": "7.2.10",
    "@angular/http": "7.2.10",
    "@angular/platform-browser": "7.2.10",
    "@angular/platform-browser-dynamic": "7.2.10",
    "@angular/router": "7.2.10",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.5.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-state": "^0.4.2",
    "bootstrap": "^4.1.0",
    "core-js": "^2.5.7",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "moment": "^2.22.2",
    "ramda": "^0.26.1",
    "ramda-extension": "^0.7.2",
    "rxjs": "^6.3.3",
    "stylelint": "^9.5.0",
    "stylelint-scss": "^3.3.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.4",
    "@angular/cli": "7.3.6",
    "@angular/compiler-cli": "7.2.10",
    "@angular/language-service": "7.2.10",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "rxjs-tslint": "0.1.5",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "3.1.3"
  }
}
