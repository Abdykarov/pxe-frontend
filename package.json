    "request": "^2.88.2",
    "rxjs": "^6.5.5",
    "smoothscroll-polyfill": "^0.4.4",
    "stylelint": "^9.5.0",
    "stylelint-scss": "^3.3.0",
    "tslib": "^2.0.0",
    "unfetch": "^4.1.0",
    "unorm": "^1.6.0",
    "uuid": "^3.3.3",
    "vanilla-text-mask": "^5.1.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "xml2js": "^0.4.23",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
