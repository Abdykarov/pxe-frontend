    "compression-webpack-plugin": "6.1.1",
    "domino": "^2.1.3",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.11",
    "protractor": "~7.0.0",
    "rxjs-tslint": "0.1.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "~6.1.0",
    "typescript": "4.0.8",
    "webpack-cli": "^3.3.0"
  },
  "browser": {
    "crypto": false
  }
}
